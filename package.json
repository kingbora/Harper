{
  "name": "harper",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prepare": "is-ci || husky install",
    "preinstall": "npx only-allow pnpm",
    "build:h5": "cross-env BUILD_PLATFORM='h5' pnpm --filter '@cus/*' run build",
    "watch:h5": "cross-env BUILD_PLATFORM='h5' pnpm --filter '@cus/*' run watch",
    "build:rn": "cross-env BUILD_PLATFORM='native' pnpm --filter '@cus/*' run build",
    "watch:rn": "cross-env BUILD_PLATFORM='native' pnpm --filter '@cus/*' run watch",
    "clear": "rimraf 'packages/*/{dist,node_modules}' && rimraf node_modules",
    "lint": "pnpm --filter '@cus/*' run lint",
    "changeset": "changeset",
    "version-packages": "changeset versions"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/core": "^7.18.5",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-native": "^0.68.2",
    "react-router-dom": "^6.3.0",
    "react-router-native": "^6.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@changesets/cli": "^2.23.0",
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/babel__core": "^7.1.19",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/glob-to-regexp": "^0.4.1",
    "@types/node": "^18.0.0",
    "@types/npmlog": "^4.1.4",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-native": "^0.67.9",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "commitizen": "^4.2.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.18.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "less": "4",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.2",
    "npmlog": "^6.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.7",
    "rollup-plugin-node-externals": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  }
}
